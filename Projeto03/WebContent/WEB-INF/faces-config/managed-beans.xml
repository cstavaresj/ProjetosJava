<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC
	"-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
	"http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
	
	<managed-bean>
		<description>MB para tela de cadastrar e tela de consultar clientes.</description>
		<managed-bean-name>ClienteMB</managed-bean-name>
		<managed-bean-class>br.uniube.mb.ClienteMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<description>MB para tela de cadastrar e tela de consultar fornecedores.</description>
		<managed-bean-name>FornecedorMB</managed-bean-name>
		<managed-bean-class>br.uniube.mb.FornecedorMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<description>MB para tela de Menu</description>
		<managed-bean-name>MenuMB</managed-bean-name>
		<managed-bean-class>br.uniube.mb.MenuMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
    
</faces-config>