<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC
	"-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
	"http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>

	
   	<navigation-rule>
		<description>Navegação do Menu</description>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>telaCadastroVirus</from-outcome>
			<to-view-id>/paginas/CadastrarVirus.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>telaConsultaVirus</from-outcome>
			<to-view-id>/paginas/ConsultarVirus.jsp</to-view-id>
			<redirect/>
		</navigation-case>
   	
		<navigation-case>
			<from-outcome>telaCadastroDoenca</from-outcome>
			<to-view-id>/paginas/CadastrarDoenca.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>telaConsultaDoenca</from-outcome>
			<to-view-id>/paginas/ConsultarDoenca.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
   	</navigation-rule>
</faces-config>	